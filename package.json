{
  "name": "hangman-jquery",
  "version": "0.0.1",
  "description": "An Express-based Hangman game with jQuery web UI",
  "main": "app.js",
  "keywords": [
    "swagger"
  ],
  "license": "MIT",
  "private": true,
  "scripts": {
    "lint": "eslint",
    "lint:fix": "eslint --fix ./test/api/**/*.js",
    "start": "node .",
    "test": "grunt test",
    "test:api": "ava",
    "test:api:watch": "ava --watch",
    "test:unit:coverage": "istanbul cover _mocha",
    "test:unit:watch": "grunt watch",
    "test:ui": "concurrently --kill-others \"npm start\" \"cypress run\""
  },
  "ava": {
    "files": [
      "test/api/*.spec.js"
    ],
    "source": [
      "test/api/*.spec.js"
    ]
  },
  "dependencies": {
    "connect": "^3.2.0",
    "file-stream-rotator": "0.0.7",
    "js-yaml": "^3.11.0",
    "lodash": "^4.12.0",
    "morgan": "^1.7.0",
    "nedb": "^1.8.0",
    "node-uuid": "^1.4.7",
    "serve-static": "^1.10.2",
    "swagger-tools": "0.9.*"
  },
  "devDependencies": {
    "ava": "^0.25.0",
    "blanket": "^1.2.3",
    "concurrently": "^3.5.1",
    "cypress": "^2.1.0",
    "eslint": "^4.19.1",
    "eslint-config-standard": "^11.0.0",
    "eslint-plugin-import": "^2.10.0",
    "eslint-plugin-node": "^6.0.1",
    "eslint-plugin-promise": "^3.7.0",
    "eslint-plugin-standard": "^3.0.1",
    "expect.js": "^0.3.1",
    "grunt": "^1.0.2",
    "grunt-contrib-jshint": "^1.0.0",
    "grunt-contrib-uglify": "^1.0.1",
    "grunt-contrib-watch": "^1.0.0",
    "grunt-karma": "^1.0.0",
    "grunt-mocha-test": "^0.12.7",
    "istanbul": "^0.4.3",
    "karma": "^0.13.22",
    "karma-coverage": "^1.0.0",
    "loadtest": "^1.3.6",
    "mocha": "^2.4.5",
    "request": "^2.85.0",
    "request-promise": "^4.2.2",
    "sinon": "^1.17.4"
  }
}
